local function getPetForEgg(v17)
local v9 = v4[v17]
if not v9 then return "?" end
local v10 = {}
for pet, chance in pairs(v9) do
if chance > 0 then table.insert(v10, pet) end
end
return #v10 > 0 and v10[math.random(1, #v10)] or "?"
end
